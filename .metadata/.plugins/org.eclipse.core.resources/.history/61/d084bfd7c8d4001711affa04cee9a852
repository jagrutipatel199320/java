package staticKeyword;

public class Student {

	int age;
	String name;
	static int NoOfStudent = 0;
	
	Student(){
		NoOfStudent++;
	}
	public int getNoOfStudenta() {
		return  NoOfStudent;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
