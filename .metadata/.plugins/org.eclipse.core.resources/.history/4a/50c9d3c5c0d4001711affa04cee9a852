package lesson1;

public class MyClassExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Student jagruti = new Student();
		
		jagruti.id = 1;
		jagruti.name = "jagruti";
		jagruti.age = 15;
		
		System.out.println(jagruti.name + "is" + jagruti.age + "years old");
	}

}
