package threadDemo;

class MyClass extends Thread{

	@Override
	public void run() {
		for(int i=0;i<10;i++) {
			System.out.println("value "+ i);
		}
	}
	
}

public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyClass myclass = new MyClass();
		myclass.start();
		
	}

}
