   <children>
      <RadioButton fx:id="rb1" layoutX="49.0" layoutY="43.0" mnemonicParsing="false" onAction="#radioChecked" selected="true" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="mygroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rb2" layoutX="48.0" layoutY="79.0" mnemonicParsing="false" onAction="#radioChecked" text="Female" toggleGroup="$mygroup" />
